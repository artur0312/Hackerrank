#include<iostream>
#include<algorithm>
#include<string>

using namespace std;

int main(){
  int n,m;
  cin>>n;
  int ranked[200000];
  for(int i=0;i<n;i++){
    cin>>ranked[i];
  }
  cin>>m;
  int player[200000];
  for(int i=0;i<m;i++){
    cin>>player[i];
  }
  int checkpoints[200000];
  checkpoints[0]=ranked[0];
  int different_scores=1;
  int same_score=1;
  for(int i=0;i<n;i++){
    if(ranked[i]<checkpoints[different_scores-1]){
      checkpoints[different_scores]=ranked[i];
      different_scores++;
    }
  }
  int worse=different_scores-1;
  for(int i=0;i<m;i++){
    cout<<worse<<endl;
    int point=player[i];
    if(point>=checkpoints[0]){
      cout<<1<<'\n';
      continue;
    }
    for(int j=worse;j>=0;j--){
      if(point<ranked[j]){
	worse=j;
	cout<<j+1<<'\n';
	break;
      }
    }
  }
}
